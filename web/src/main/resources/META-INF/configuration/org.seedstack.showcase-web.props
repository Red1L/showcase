####################################################################
# web security
####################################################################
[org.seedstack.seed.security.urls]
/** = authcBasic

#[org.seedstack.seed.jms]
#connection-factories = cf1
#connections = c1

#[org.seedstack.seed.jms.connection-factory.cf1]
#vendor.class = com.ibm.mq.jms.MQConnectionFactory
#vendor.property.queueManager = YVASA960A
#vendor.property.hostName = yvasa960.inetpsa.com
#vendor.property.channel = YVASA960A.MQMON.CLNT
#vendor.property.port = 1414
#vendor.property.transportType = 1

#[org.seedstack.seed.jms.connection.c1]
#connection-factory = cf1
#managed-connection = true
#polling = true

####################################################################
# web-services
####################################################################
[org.seedstack.seed.ws]
endpoints = ProductInfoWS, HelloService
endpoint.ProductInfoWS.implementation = org.seedstack.showcase.ws.productinfo.ProductInfoServiceWebImpl
endpoint.ProductInfoWS.wsdl = ProductInfoWS.wsdl
endpoint.ProductInfoWS.url = /ws/productInfo
endpoint.ProductInfoWS.enable-mtom = true

endpoint.HelloService.implementation = org.seedstack.showcase.ws.hello.HelloService
endpoint.HelloService.wsdl = Hello.wsdl
endpoint.HelloService.binding = http://www.w3.org/2010/soapjms/
#endpoint.HelloService.url = jms:queue:TEST_QUEUE.1?connectionName=c1
#endpoint.HelloService.url = jms:jndi:jms/HelloQueue?jndiConnectionFactoryName=jms/ShowcaseConnectionFactory
endpoint.HelloService.url = jms:jndi:java:comp/env/jms/HelloQueue?jndiConnectionFactoryName=java:comp/env/jms/ShowcaseConnectionFactory
endpoint.HelloService.jms.pooling = true
endpoint.HelloService.jms.set-client-id = false 


